let or'  : bool = T or T;
let and' : bool = T and T;
let xor' : bool = T xor T;

let feq : bool = 0 == 0;
let fle : bool = 0 <= 1;
let flt : bool = 0 <  1;
let fge : bool = 1 >= 0;
let fgt : bool = 1 >  0;

let add : field = 1 + 1;
let sub : field = 1 - 1;
let mul : field = 1 * 1;
let div : field = 1 / 1;

let bat : bool = { T } [ 0 ];
